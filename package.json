{
  "name": "node-boilerplate-api",
  "version": "1.0.0",
  "description": "Boilerplate project for Nodejs API's",
  "author": "Christiaan Maks <christilut@gmail.com>",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "start": "tsc index.ts",
    "debug": "cross-env NODE_ENV=development tsc index.ts",
    "dev": "cross-env NODE_ENV=development nodemon --exec 'ts-node' --ext ts,gql index.ts",
    "deploy": "git push heroku master",
    "lint": "esw *.js server config --color",
    "lint:watch": "npm run lint -- --watch",
    "precommit": "npm run lint && npm run test",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --bail --timeout 1200000 --exit --ui bdd --reporter spec --colors server/tests --recursive",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --timeout 12000 --ui bdd --reporter spec --colors --compilers js:babel-core/register server/tests --recursive",
    "test:check-coverage": "npm run test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Christilut/node-api-boilerplate.git"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "es2017",
    "API",
    "boilerplate",
    "graphql",
    "typeorm",
    "postgres",
    "sql",
    "typescript"
  ],
  "dependencies": {
    "@decorators/mongoose": "^2.0.1",
    "@types/joi": "^10.4.3",
    "@types/mongoose": "^4.7.24",
    "@types/node": "^8.0.41",
    "@types/winston": "^2.3.6",
    "apollo-server-express": "^1.1.7",
    "app-module-path": "2.2.0",
    "ava": "0.22.0",
    "aws-sdk": "^2.134.0",
    "axios": "0.16.2",
    "babel-eslint": "8.0.1",
    "bcrypt": "1.0.3",
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "chai-date-string": "0.1.0",
    "compression": "1.7.1",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "debug": "3.1.0",
    "dotenv": "^4.0.0",
    "eslint-config-standard": "10.2.1",
    "express": "^4.16.2",
    "express-jwt": "5.3.0",
    "express-promise-router": "^2.0.0",
    "express-rate-limiter": "^1.3.1",
    "express-winston": "2.4.0",
    "fs-extra": "4.0.2",
    "graphql": "^0.11.7",
    "graphql-tools": "^2.5.1",
    "handlebars": "4.0.10",
    "helmet": "^3.9.0",
    "http-status": "1.0.1",
    "joi": "^13.0.0",
    "joi-objectid": "^2.0.0",
    "jsonwebtoken": "^8.1.0",
    "jwt-decode": "2.2.0",
    "mailcomposer": "4.0.2",
    "mailgun-js": "0.13.1",
    "merge-graphql-schemas": "^1.1.4",
    "method-override": "2.3.10",
    "mockgoose": "^7.3.3",
    "mongoose": "^4.12.3",
    "morgan": "1.9.0",
    "nodemon": "1.12.1",
    "nyc": "11.2.1",
    "passport": "0.4.0",
    "passport-jwt": "3.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.3.0",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.10",
    "ts-node": "^3.3.0",
    "tslint": "5.7.0",
    "tslint-config-standard": "6.0.1",
    "typescript": "^2.5.3",
    "url-parse": "1.1.9",
    "winston": "^2.4.0",
    "winston-cloudwatch": "1.12.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "commitizen": "^2.9.2",
    "coveralls": "3.0.0",
    "cross-env": "^5.1.0",
    "cz-conventional-changelog": "2.0.0",
    "del": "3.0.0",
    "husky": "0.14.3",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "4.0.1",
    "run-sequence": "2.2.0",
    "supertest": "3.0.0",
    "validate-commit-msg": "2.14.0"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "es2016",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime",
      "add-module-exports",
      "transform-async-to-generator"
    ]
  }
}
